{
  "manifest_version": 2,
  "name": "GTmetrix Analyzer Plugin",
  "description": "This extension will analyze a 
page using GTmetrix",
  "version": "1.0",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
 ],
 "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/highlight.min.js 'self'"
}
